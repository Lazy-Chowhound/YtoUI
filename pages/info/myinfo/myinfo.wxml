<view class='table'>
  <view class='tr'>
    <view class='th'>姓名</view>
    <view class='th'>性别</view>
    <view class='th'>年龄</view>
    <view class='th'>电话</view>
    <view class='th'>工号</view>
  </view>
  <block wx:key="item" wx:for="{{myinfo}}">
    <view class='tr'>
      <view class='td'>{{item.name}}</view>
      <view class='td'>{{item.sex}}</view>
      <view class='td'>{{item.age}}</view>
      <view class='td'>{{item.phone}}</view>
      <view class='td'>{{item.employId}}</view>
    </view>
  </block>
</view>
<view class="weui-form-preview__ft">
  <navigator url="../organize/organize" class="weui-form-preview__btn weui-form-preview__btn_primary" hover-class="weui-form-preview__btn_active">修改    </navigator>
</view>